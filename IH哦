import java.util.*;
class Square{//创建类
	int a;//成员变量
	int getArea() {//构造方法
		return a*a;
	}
}
public class Main{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		while(sc.hasNext()) {
		Square sq=new Square();//声明并创建对象
		int s;
		sq.a=sc.nextInt();
		s=sq.getArea();//使用对象
		System.out.println(s);
		}
		sc.close();
	}
}
